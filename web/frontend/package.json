{
  "name": "rulek-web-frontend",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:unit": "vitest run",
    "coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:phase0": "playwright test tests/phase0-simple.spec.ts",
    "test:phase1": "playwright test ../../tests/e2e/phase1.spec.ts",
    "test:phase2": "playwright test ../../tests/e2e/phase2.spec.ts",
    "test:phase3": "playwright test ../../tests/e2e/phase3.spec.ts",
    "test:phase4": "playwright test ../../tests/e2e/phase4.spec.ts",
    "test:phase5": "playwright test ../../tests/e2e/phase5.spec.ts",
    "test:phase6": "playwright test ../../tests/e2e/phase6.spec.ts",
    "setup:playwright": "npx playwright install",
    "setup:deps": "npm install",
    "track:phase0": "cd ../.. && node scripts/track-progress.js 0",
    "track:phase1": "cd ../.. && node scripts/track-progress.js 1",
    "track:phase2": "cd ../.. && node scripts/track-progress.js 2",
    "track:phase3": "cd ../.. && node scripts/track-progress.js 3",
    "track:phase4": "cd ../.. && node scripts/track-progress.js 4",
    "track:phase5": "cd ../.. && node scripts/track-progress.js 5",
    "track:phase6": "cd ../.. && node scripts/track-progress.js 6",
    "report:phase0": "cd ../.. && python scripts/progress_reporter.py 0",
    "report:phase1": "cd ../.. && python scripts/progress_reporter.py 1",
    "report:phase2": "cd ../.. && python scripts/progress_reporter.py 2",
    "report:phase3": "cd ../.. && python scripts/progress_reporter.py 3",
    "report:phase4": "cd ../.. && python scripts/progress_reporter.py 4",
    "report:phase5": "cd ../.. && python scripts/progress_reporter.py 5",
    "report:phase6": "cd ../.. && python scripts/progress_reporter.py 6"
  },
  "dependencies": {
    "@vicons/ionicons5": "^0.12.0",
    "axios": "^1.6.2",
    "dayjs": "^1.11.10",
    "naive-ui": "^2.35.0",
    "pinia": "^2.1.7",
    "socket.io-client": "^4.5.4",
    "vue": "^3.3.11",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@playwright/test": "^1.54.2",
    "@types/node": "^20.10.5",
    "@vitejs/plugin-vue": "^4.5.2",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vitest/ui": "^1.1.0",
    "@vue/tsconfig": "^0.5.0",
    "autoprefixer": "^10.4.16",
    "chalk": "^5.3.0",
    "eslint": "^8.56.0",
    "playwright": "^1.54.2",
    "postcss": "^8.4.32",
    "prettier": "^3.1.1",
    "sass": "^1.69.5",
    "tailwindcss": "^3.4.0",
    "typescript": "~5.3.0",
    "vite": "^5.0.10",
    "vitest": "^1.1.0",
    "vue-tsc": "^1.8.27"
  }
}
